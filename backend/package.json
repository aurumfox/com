{
  "name": "aurum-fox-backend-ultimate",
  "version": "1.0.0",
  "description": "The ultimate backend for Aurum Fox Unified Portal, combining Web2 and Web3 functionalities.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --detectOpenHandles --forceExit --runInBand",
    "lint": "eslint .",
    "seed": "node scripts/seed.js",
    "stop-db": "docker-compose down"
  },
  "keywords": [
    "solana",
    "web3",
    "backend",
    "nodejs",
    "express",
    "mongodb",
    "redis",
    "graphql",
    "nft",
    "marketplace",
    "dapp"
  ],
  "author": "Aurum Fox Team",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-kms": "^3.609.0",
    "@aws-sdk/client-secrets-manager": "^3.609.0",
    "@solana/web3.js": "^1.91.0",
    "awilix": "^10.0.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^7.3.0",
    "express-rate-limit-redis": "^3.0.0",
    "express-xss-clean": "0.1.4",
    "graphql": "^15.9.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "mongoose-morgan": "^1.0.18",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.15",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "supertest": "^7.0.0"
  }
}
